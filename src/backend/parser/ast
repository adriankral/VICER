{
    type: "translation_unit",
    external_declarations:
        [function_definition | declaration]
}

TVALUE = function | pointer | concrete_type

{
    type: "function_definition",
    name: "mariuszKkK",
    prototype: function,
    param_names: [String],
    body: compound_statement
}

{
    type: "declaration",
    name: "adrian_kral__yoyo_",
    tvalue: TVALUE,
}

{
    type: "concrete_type",
    name: "int" | "void" | "char" .. (na razie tylko "int")
}

{
    type: "pointer",
    tvalue: TVALUE
}

{ 
    type: "function",
    param_tvalues: [TVALUE],
    return_tvalue: TVALUE,
    is_variadic: true | false
}

STATEMENT = ...

{
    type: "compound_statement",
    declarations: [declaration],
    statements: [STATEMENT]
}

{
    type: "expression_statement",
    expression: ...
}

{ type: "BREAK" }

{ type: "CONTINUE" }

{ 
    type: "RETURN",
    rexpression: ...;
}

{
    type: "IF",
    condition: ...,
    true_body: STATEMENT,
    false_body: STATEMENT,
}

{
    type: "WHILE"
    condition: ...,
    body: STATEMENT
}
